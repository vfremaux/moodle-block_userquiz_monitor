

<!-- examcategoryresult -->
<a id="id-cat-{{catid}}" name="name-cat-{{catid}}"></a>
<div class="trans100 {{islastclass}} div-main" id="id-div-main-{{catid}}">
    <div class="userquiz-monitor-categorycontainer">

        <div class="userquiz-monitor-main-item">
            <div class="userquiz-monitor-row">

                <div class="userquiz-monitor-cell categoryname"><label for="id-cb-master-{{catid}}">{{name}}</label></div>
{{#hassubs}}
                <div class="userquiz-monitor-cell details-button" id="id-details-button-{{catid}}">
                    <label for="radio-main-item-{{catid}}">
{{#pixurl}}
                        <img class="userquiz-monitor-exam-cat-button"
                              title="{{seesubsstr}}"
                              src="{{{pixurl}}} "/>
{{/pixurl}}
{{^pixurl}}
                        <div id="details-button-div-{{catid}}" class="userquiz-monitor-exam-cat-button">{{seesubsstr}}</div>
{{/pixurl}}
                    </label>
               </div>
{{/hassubs}}
            </div>

            <div class="userquiz-monitor-row">

                <div class="userquiz-monitor-cell">
                    {{{accessorieslink}}}
                    <input type="hidden" name="h_cb_pl{{catid}}" value="h_cb_pl{{catid}}"/>
                </div>

                <div class="userquiz-monitor-cell"><!-- Blank cell -->
{{#qdebug}}
                <div class="qdebug">
                    <pre>
                    {{{qdebug}}}
                    </pre>
                </div>
{{/qdebug}}
                </div>

            </div>

            <div class="category-bargraph">
                <table width="100%">

                    {{{barheadrow}}}
                    {{{barrangerowA}}}
                    {{{barrangerowC}}}

                </table>
            </div>

        </div>
        <input id="radio-main-item-{{catid}}" type="radio" name="radio-main-item" class="radio-hidden" value="{{catid}}" />
        <div id="category-subcatpod-{{catid}}" class="category-subpod">
            <!-- container for subcategories --><img src="{{{loadingurl}}}" />
        </div>
    </div>
</div>
<!-- /examcategoryresult -->