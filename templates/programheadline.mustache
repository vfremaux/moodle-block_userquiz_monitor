{{!
    @template block_userquiz_monitor/programheadline

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none
}}
<!-- block_userquiz_monitor/programheadline -->
<div id="userquiz-monitor-program-headline">
    <div class="userquiz-monitor-row">
        <div class="userquiz-monitor-cell">
            <h1>{{{catstr}}}</h1>
        </div>
    </div>
</div>

{{#selectall}}
<div class="trans100">
    <div class="userquiz-monitor-main-item" id="userquiz-select-all">
        <div class="userquiz-monitor-row">
            <div class="userquiz-monitor-cell">
                <input type="checkbox"
                        name="checkallmaster"
                        id="checkall-master"
                        style="padding-left:2px;"/>
                <span class="select-all">{{{selectallcbstr}}}</span>
            </div>
        </div>
    </div>
</div>
{{/selectall}}
<!-- /block_userquiz_monitor/programheadline -->
