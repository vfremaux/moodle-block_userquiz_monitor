{{!
    @template block_userquiz_monitor/exam

    Template displaying the exam detailed results panel.

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none
}}
<!-- exam -->
<div id="userquiz-subpod-switch"></div>

    <input type="hidden" name="blockid" value="{{blockid}}">

    {{{globalmonitor}}}

    <div class="userquiz-monitor-trainingcontainer container-fluid">

{{#errors}}
        {{{initerrorstr}}}
        {{{compileerrorstr}}}
{{/errors}}

{{^errors}}

        <div class="userquiz-monitor-container-row row-fluid">
            <div class="userquiz-monitor-area>

                {{{programheadline}}}

                {{#categoryresults}}
                {{{result}}}
                {{/categoryresults}}

                <div id="rootcatnotes">
                    {{{note1}}}
                    {{{note2}}}
                </div>

            </div>
        </div>

{{/errors}}

    </div>

</div>
<!-- /exam -->
